<?xml version="1.0" encoding="UTF-8"?>
<a:authorAction xmlns:a="http://www.oxygenxml.com/ns/author/external-action"
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:schemaLocation="http://www.oxygenxml.com/ns/author/external-action http://www.oxygenxml.com/ns/author/external-action/authorAction.xsd"
  id="insert.table.fragment">
  <a:name>${i18n(insert.table)}</a:name>
  <a:description>${i18n(insert.table_description)}</a:description>
  <a:largeIconPath href="/images/Table24.png"/>
  <a:smallIconPath href="/images/Table16.png"/>
  <a:operations>
    <a:operation id="InsertFragmentOperation">
      <a:xpathCondition>oxy:allows-global-element("*", "class", " topic/table ")</a:xpathCondition>
      <a:arguments>
        <a:argument name="fragment">
<table frame="all" colsep="1" rowsep="1">
  <title/>
  <tgroup cols="2">
    <colspec colname="c1" colnum="1" colwidth="1*"/>
    <colspec colname="c2" colnum="2" colwidth="1*"/>
    <thead>
      <row>
        <entry/>
        <entry/>
      </row>
    </thead>
    <tbody>
      <row>
        <entry/>
        <entry/>
      </row>
      <row>
        <entry/>
        <entry/>
      </row>
      <row>
        <entry/>
        <entry/>
      </row>
    </tbody>
  </tgroup>
</table>
        </a:argument>
      </a:arguments>
    </a:operation>
  </a:operations>
  <a:accessKey/>
</a:authorAction>
